{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "strict": true,
    "moduleResolution": "node",
    "module": "CommonJS",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true
  },
  "include": ["**/*.ts"]
}
